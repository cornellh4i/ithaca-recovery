datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  uid  String @unique
  name String
}

model Admin {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  uid           String @unique
  name          String
  email         String @unique
}

model Meeting {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  mid            String   @unique
  title          String
  description    String
  creator        String   
  group          String   
  startDateTime  DateTime
  endDateTime    DateTime
  email          String
  zoomAccount    String?   
  zoomLink       String?
  zid            String?
  type           String
  room           String
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  uid     String   @unique
  name    String?
}

model Admin {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  uid     String   @unique
  name    String?
  email   String   @unique
  pmode   String 
}
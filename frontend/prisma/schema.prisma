datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  uid     String   @unique
  name    String?
}

model Meeting {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  mid          String   @unique
  title        String
  description  String
  creator      String   
  group        String   
  date         DateTime
  startTime    DateTime
  fromTime     DateTime
  zoomAccount  String   
}
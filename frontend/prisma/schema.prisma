generator client {
  provider = "prisma-client-js"
}

model Users {
  id      Int     @id @default(autoincrement())
  uid     String  @unique
  name    String?
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL") // connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // direct connection
}